{
  "name": "kiai-skeleton",
  "version": "1.0.0",
  "description": "Boilerplate and example code for use with the Kiai VoiceAction Framework",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:ts && rm -rf dist/ && mkdir -p dist && cp -r config flows lib index.js package.json dist/",
    "dev": "npm-run-all --parallel dev:ts dev:local",
    "dev:local": "nodemon index.js --local --port `node ./tools/port.js`",
    "dev:firebase": "firebase serve --only functions --port 3000",
    "deploy:gcloud": "rm -f deploy.zip; zip -rXx node_modules,.git,assets deploy.zip *",
    "deploy:firebase": "firebase deploy",
    "deploy": "npm run deploy:firebase",
    "dev:ts": "tsc --watch",
    "dev:port": "node tools/setPort.js",
    "build:ts": "tsc",
    "sync": "node ./tools/sync/index.js"
  },
  "dependencies": {
    "@google-cloud/datastore": "^1.4.2",
    "kiai": "^2.3.0",
    "lodash": "^4.17.11",
    "minimist": "^1.2.0"
  },
  "devDependencies": {
    "colors": "^1.3.3",
    "columnify": "^1.5.4",
    "deep-diff": "^1.0.2",
    "dialogflow": "^0.7.0",
    "eslint": "^5.8.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^2.10.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-typescript": "^1.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^2.7.0",
    "firebase-admin": "^6.1.0",
    "is-running": "^2.1.0",
    "ngrok": "^3.1.0",
    "node-fs-extra": "^0.8.2",
    "nodemon": "^1.18.6",
    "npm-run-all": "^4.1.3",
    "portfinder": "^1.0.20",
    "prettier": "^1.14.3",
    "prompt-confirm": "^2.0.4",
    "tslint": "^5.12.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.1.6"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:mediamonks/kiai-skeleton.git"
  },
  "author": {
    "name": "Richard Heuser",
    "email": "richard.heuser@mediamonks.com"
  },
  "license": "MIT",
  "engines": {
    "node": "8"
  }
}
